# Environment variables for GeneForgeLang development container

# Python settings
PYTHONPATH=/workspace
PYTHONUNBUFFERED=1

# Application settings
GFL_ENV=development
GFL_DEBUG=true
GFL_LOG_LEVEL=DEBUG

# Database settings (for testing)
DATABASE_URL=postgresql://gfl_user:gfl_password@postgres:5432/gfl_test

# API settings
API_HOST=0.0.0.0
API_PORT=8000

# Web interface settings
WEB_HOST=0.0.0.0
WEB_PORT=8080

# Security settings
SECRET_KEY=dev-secret-key-change-in-production
